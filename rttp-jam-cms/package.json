{
  "name": "rttp-jam-cms",
  "version": "1.0.0",
  "private": true,
  "description": "Sanity CMS UI for the RTTP results page.",
  "main": "package.json",
  "scripts": {
    "start": "sanity start",
    "test": "sanity check"
  },
  "comments": {
    "yarn.lock": "Apparently, `sanity install` must use yarn. Instead of running `npm install/start`, simply use `sanity install/start` instead."
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "prettier/react"
    ]
  },
  "prettier": {
    "semi": false
  },
  "dependencies": {
    "@sanity/base": "^2.10.5",
    "@sanity/code-input": "^2.9.0",
    "@sanity/color-input": "^2.2.6",
    "@sanity/components": "^2.2.6",
    "@sanity/core": "^2.10.5",
    "@sanity/dashboard": "^2.10.5",
    "@sanity/default-layout": "^2.10.5",
    "@sanity/default-login": "^2.10.5",
    "@sanity/desk-tool": "^2.10.5",
    "@sanity/vision": "^2.10.5",
    "prop-types": "^15.6",
    "react": "^17",
    "react-dom": "^17",
    "react-icons": "^4.1.0",
    "sanity-plugin-content-calendar": "^1.1.0",
    "sanity-plugin-graph-view": "^1.0.4",
    "sanity-plugin-latex-input": "^1.0.1",
    "sanity-plugin-markdown": "^2.0.2"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  }
}
