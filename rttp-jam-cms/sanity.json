{
  "root": true,
  "api": {
    "projectId": "vo995g7g",
    "dataset": "production"
  },
  "project": {
    "name": "RTTP Jam"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/dashboard",
    "content-calendar",
    "@sanity/color-input",
    "@sanity/code-input",
    "markdown",
    "latex-input",
    "dashboard-widget-gatsby"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./deskStructure.js"
    },
    {
      "implements": "part:@sanity/dashboard/config",
      "path": "./dashboardConfig.js"
    },
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    }
  ]
}
